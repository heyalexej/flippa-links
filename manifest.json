{
    // Required
  "manifest_version": 2,
  "name": "Flippa Links",
  "version": "0.1",
  "description": "Adds direct links to the websites listed on flippa.com ",
  "permissions": [
    "tabs", "https://flippa.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://flippa.com/*"],
      "js": ["script.js"]
    }
  ]
}
